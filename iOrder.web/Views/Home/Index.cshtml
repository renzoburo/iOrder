@{
    ViewData["Title"] = "Home";
}
<div class="row">
    <table>
        <thead>
            <tr>
                <th>First Names</th>
                <th>Surname</th>
                <th>Address Type</th>
                <th>Street Address</th>
                <th>Suburb</th>
                <th>City</th>
                <th>Postal Code</th>
                <td></td>
                <td></td>
            </tr>
        </thead>
        <tbody data-bind="foreach: clients">
            <tr>
                <td><input data-bind="value: firstNames" /></td>
                <td><input data-bind="value: surname" /></td>
                <td><input data-bind="value: addressType" /></td>
                <td><input data-bind="value: streetAddress" /></td>
                <td><input data-bind="value: suburb" /></td>
                <td><input data-bind="value: city" /></td>
                <td><input data-bind="value: postalCode" /></td>
                <td><button data-bind="click: saveClient" />Save</td>
                <td><button data-bind="click: getClient" />Order</td>
            </tr>
        </tbody>
    </table>
    <button data-bind="click: addClient">Add Client</button>
    <button data-bind="click: removeClient">Remove Client</button>
</div>
<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"></script>
<script src="/scripts/knockout-3.4.2.debug.js"></script>
<script src="scripts/Client.js"></script>
